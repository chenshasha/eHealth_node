<%- include header %>
 <div style="padding:50px">
 </div>

 <div class="container">
 <div>
 <form method="post" action="/newpost">
 Post Title:
 <div style="padding:5px"></div>
 <input name = "title" type="text" class="form-control" placeholder="Title">
 <div style="padding:5px"></div>
 <textarea name = "content" class="form-control" rows="10" placeholder="Type Here ..."></textarea>
 <div style="padding:5px"></div>
 <div class="radio">
   <label>
     <input type="radio" name = "postType" id="success" value="success" checked>
     Q and A
   </label>
 </div>
 <div class="radio">
   <label>
     <input type="radio" name = "postType" id="info" value="info">
     Health Tip
   </label>
 </div>
 <div style="padding:5px"></div>
 <button type="submit" class="btn btn-primary" style="float: left;">Submit </button>
 </form>
 </div>
 </div>