 <%- include header %>
 <!-- messi message -->
   	<script>
          	jQuery.noConflict();
         	(function($) {
                      $(document).ready(function() {
                            new Messi('You can modify your profile, choose to make an appointment, or go to the forum', {autoclose: 6000, center: false, viewport: {top: '350px', left: '450px'}});

                                  });
                      })(jQuery);

    </script>
<div style="padding:30px">
  </div>
<div class="container">

	<div class="page-header text-center">

		<h1><span class="fa fa-anchor"></span> Profile Page</h1>
	</div>


	<form>
    <div class="col-md-6 col-md-offset-3">
    		  <table class="table table-bordered table-striped">
    	          <tbody style="text-align:left;">
    			    <tr>
    				    <th style="width: 150px;">Email: </th>
    				    <td><%= user.local.email %><br></td>
    			    </tr>
    			    <tr>
                        <th style="width: 150px;">Name: </th>
                    	<td><%= person.firstName %> <%= person.lastName %></td>
                    </tr>
    			    <tr>
                        <th style="width: 150px;">Phone Number: </th>
                    	<td><%= person.phone %><br></td>
                    </tr>
    			    <tr>
                        <th style="width: 150px;">Address: </th>
                    	<td><%= person.address %></td>
                    </tr>

    		    </tbody>
    	    </table>
    	    <a href="/modifyprofile" class="btn btn-primary btn-sm">Modify   </a>
    </form>


</div>

