 <%- include header %>
 <!-- messi message -->
  	<script>
      	jQuery.noConflict();
      	(function($) {
                $(document).ready(function() {
                  new Messi('You can modify your profile, choose to make an appointment, or go to the forum', {autoclose: 6000, center: false, viewport: {top: '200px', left: '600px'}});

                   });


          })(jQuery);

          </script>



<div style="padding:30px">
  </div>
<div class="container">

	<div class="page-header text-center">

		<h1><span class="fa fa-anchor"></span> Profile Page</h1>
	</div>

	<div class="row">

		<!-- LOCAL INFORMATION -->
		<div class="col-sm-6">
			<div class="well">
				<h3><span class="fa fa-user"></span> Local</h3>


					<p>
						<strong>Email</strong>: <%= user.local.email %><br>
						<strong>User Type</strong>: <%= user.local.userType %><br>
